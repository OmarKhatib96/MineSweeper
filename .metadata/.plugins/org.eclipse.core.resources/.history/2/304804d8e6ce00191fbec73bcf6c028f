

 enum lvl {
	    EASY,
	    MEDIUM,
	    HARD
	}
public class Level {

	
	
	private lvl level;
	
	public Level()//Constructeur par défaut
	{
		
		
	}
	
	public Level(lvl l)//Constructeur surchargé
	{
		
	level=lvl;	
	}
	
	public void setLevel(int lvl)//level setter
	{
		level=lvl;
	}
	public int getLevel()//level getter
	{
		
		return level;
		
	}
	//Destructeur
	protected void finalize() throws Throwable
	{
		super.finalize();
	}
			
	
	
}
