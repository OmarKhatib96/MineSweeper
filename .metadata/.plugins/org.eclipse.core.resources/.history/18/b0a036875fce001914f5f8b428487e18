
public class Demineur {
	
	
	
	public Demineur()
	{
		
		//Champ crée avec premier constructeur par défaut
    	Champ C=new Champ();    	
    	C.placeMines();
    	C.affText();    	
    	C.toString();//compiler writes here s1.toString()     	
    	System.out.println(C.toString());//Affichage de C(après surcharge)
				
	}
	
	
    public static void main(String args []){
    	new Demineur();
  //Champ crée avec premier constructeur par défaut
    }
    
}
